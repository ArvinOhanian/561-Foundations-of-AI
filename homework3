def Pos_Int(string):#Turns a string into an int list as long as the OG string has a number every other index ex. String = ["1 2 3"]
    Position = []
    count = 0
    for number in string:
        if(count%2 == 0):
            Position.append(int(string[count]))
        count+=1    
    
    return Position

def Move(Loc1, Loc2):#adds two lists element wise. Lists must be same dimenions and only one row
    Loc3 = []
    for i in range(0, len(Loc1)): 
        Loc3.append(Loc1[i] + Loc2[i]) 
    return Loc3   
     
if __name__ == '__main__':
    main()
